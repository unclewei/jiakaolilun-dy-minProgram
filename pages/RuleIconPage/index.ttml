<page-meta page-font-size="{{fontSize || 16}}px" root-font-size="{{fontSize || 16}}px">
  <view class='init'>
    <view class='wrap'>
      <view tt:for="{{ruleIconTypeList}}" tt:key='_id' tt:for-item="i" class='waysItem' bindtap="gotoPage" data-item="{{i}}">
        <view class='wayTextWrap'>
          <view class='title'>
            <view class='wayText'>{{i.name}}</view>
            <view class='numWrap'>
              <view class='nums'>{{i.iconCount}}å¼ </view>
              <image class='iconRight' src='../../images/icon/arrow-right.png' mode="aspectFit" />
            </view>
          </view>
          <view class='iconWrap'>
            <image class='iconImg' tt:for="{{i.ruleIconList}}" tt:for-item="o" tt:for-index="imgIndex" src='{{urlPrefix + o.imgPath}}' mode="aspectFit" catchtap="onShowGallery" data-images="{{i.ruleIconList}}" data-item="{{o}}" />
          </view>
        </view>
      </view>
    </view>
  </view>
</page-meta>