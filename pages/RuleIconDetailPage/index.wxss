.init {
  height: fit-content;
  display: flex;
  align-items: center;
  flex-direction: column;
  background-color: #f1f1f1;
}
.wrap {
  width: 100%;
  display: flex;
  justify-content: space-around;
  align-items: center;
  flex-direction: row;
  flex-wrap: wrap;
  padding: 5px;
  box-sizing: border-box;
}
.waysItem {
  width: 49%;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: row;
  border-radius: 8px;
  margin-bottom: 5px;
  background-color: #fff;
  padding: 10px;
  box-sizing: border-box;
}
.wayTextWrap {
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.iconImg {
  width: 100px !important;
  height: 100px !important;
  object-fit: contain;
}
.title {
  width: 100%;
  height: 55px;
  font-size: 13px;
  color: #333;
  font-weight: 600;
  text-align: center;
}
